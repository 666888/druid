{
    "type" : "index",
    "dataSource" : "wikipedia_index_test",
    "granularitySpec" : {
        "type" : "uniform",
        "gran" : "DAY",
        "intervals" : [ "2013-08-31/2013-09-01" ]
    },
    "aggregators" : [{
                         "type" : "count",
                         "name" : "count"
                     }, {
                         "type" : "doubleSum",
                         "name" : "added",
                         "fieldName" : "added"
                     }, {
                         "type" : "doubleSum",
                         "name" : "deleted",
                         "fieldName" : "deleted"
                     }, {
                         "type" : "doubleSum",
                         "name" : "delta",
                         "fieldName" : "delta"
                     }],
    "firehose" : {
        "type" : "local",
        "baseDir" : "/resources/indexer",
        "filter" : "wikipedia_index_data.json",
        "parser" : {
            "timestampSpec" : {
                "column" : "timestamp"
            },
            "data" : {
                "format" : "json",
                "dimensions" : ["page","language","user","unpatrolled","newPage","robot","anonymous","namespace","continent","country","region","city"]
            }
        }
    }
}