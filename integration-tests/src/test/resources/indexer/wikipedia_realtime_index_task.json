{
    "type": "index_realtime",
    "schema": {
        "dataSource": "wikipedia_index_test",
        "indexGranularity" : "second",
        "aggregators": [
            {
                "type": "count",
                "name": "count"
            },
            {
                "type": "doubleSum",
                "name": "added",
                "fieldName": "added"
            },
            {
                "type": "doubleSum",
                "name": "deleted",
                "fieldName": "deleted"
            },
            {
                "type": "doubleSum",
                "name": "delta",
                "fieldName": "delta"
            }
        ]
    },
    "firehose": {
        "type":"timed",
        "shutoffTime":"#SHUTOFFTIME",
        "delegate" :{
        "type": "receiver",
        "serviceName": "eventReceiverServiceName",
        "bufferSize": 100000,
        "parser": {
            "timestampSpec": {
                "column": "timestamp",
                "format": "iso"
            },
            "dimensions": [
                "page", "language", "user", "unpatrolled", "newPage", "robot", "anonymous",
                "namespace", "continent", "country", "region", "city"
            ]
        }
    }},
    "fireDepartmentConfig": {
        "maxRowsInMemory": 1,
        "intermediatePersistPeriod": "PT1M"
    },
    "windowPeriod": "PT1M",
    "segmentGranularity": "DAY",
    "rejectionPolicy": {
        "type": "none"
    }

}
